{
  "name": "sunset_inventory_frontend",
  "version": "1.0.0",
  "description": "Frontend Inventory Management System",
  "main": "./src/index.js",
  "repository": "https://github.com/WERNSA/sunset_inventory_frontend.git",
  "author": "Jorge Antonio Valverde Mejia <jorgeavalverdem@hotmail.com>",
  "license": "BSD-3-Clause",
  "private": false,
  "scripts": {
    "start": "npm run watch:css && webpack serve --hot --port 3000",
    "build": "npm run build:css && webpack --config webpack.config.js --mode production",
    "build:css": "postcss src/styles/index.css -o dist/output.css",
    "watch:css": "postcss src/styles/index.css -o src/styles/tailwind.css",
    "lint": "eslint --ext .jsx,.js src",
    "pre-commit": "npm run lint",
    "pre-push": "npm test",
    "post-merge": "npm install",
    "post-rewrite": "npm install"
  },
  "dependencies": {
    "antd": "^5.1.6",
    "axios": "^1.5.0",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.20.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@svgr/webpack": "^6.5.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
